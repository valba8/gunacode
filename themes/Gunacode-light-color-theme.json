{
  "name": "Gunacode Light",
  "type": "light",
  "colors": {
    "foreground": "#6E7580",
    "descriptionForeground": "#8C8C8C",
    "errorForeground": "#FF1919",
    "focusBorder": "#ED9366",
    "selection.background": "#ED936633",
    "editor.background": "#F7F4EE",
    "editor.foreground": "#6E7580",
    "editorLineNumber.foreground": "#ABB0B6",
    "editorLineNumber.activeForeground": "#6E7580",
    "editorCursor.foreground": "#ED9366",
    "editor.selectionBackground": "#ED936633",
    "editor.selectionHighlightBackground": "#ED936655",
    "editor.inactiveSelectionBackground": "#E6E0D6",
    "editor.lineHighlightBackground": "#EEE8DE",
    "editor.wordHighlightBackground": "#ED936640",
    "editor.wordHighlightStrongBackground": "#ED936666",
    "editor.findMatchBackground": "#ED936699",
    "editor.findMatchForeground": "#000000",
    "editor.findMatchHighlightBackground": "#ED936655",
    "editor.findRangeHighlightBackground": "#ED936633",
    "editorBracketMatch.border": "#0095B3",
    "editorBracketMatch.background": "#0095B31A",
    "editorIndentGuide.background": "#DDD7CE",
    "editorIndentGuide.activeBackground": "#ABB0B6",
    "editorWhitespace.foreground": "#DDD7CE",
    "editorGutter.background": "#F7F4EE",
    "editorGutter.added": "#2F8D6E",
    "editorGutter.modified": "#0095B3",
    "editorGutter.deleted": "#FF1919",
    "diffEditor.insertedTextBackground": "hsla(160, 35%, 40%, 0.14)",
    "diffEditor.insertedLineBackground": "hsla(160, 35%, 40%, 0.14)",
    "diffEditor.removedTextBackground": "hsla(5, 45%, 55%, 0.14)",
    "diffEditor.removedLineBackground": "hsla(5, 45%, 55%, 0.14)",
    "gitDecoration.addedResourceForeground": "#2F8D6E",
    "gitDecoration.modifiedResourceForeground": "#0095B3",
    "gitDecoration.deletedResourceForeground": "#FF1919",
    "gitDecoration.untrackedResourceForeground": "#2F8D6E",
    "gitDecoration.ignoredResourceForeground": "#8C8C8C",
    "list.activeSelectionBackground": "#E6E0D6",
    "list.activeSelectionForeground": "#4B4B4B",
    "list.inactiveSelectionBackground": "#E6E0D6",
    "list.inactiveSelectionForeground": "#4B4B4B",
    "list.hoverBackground": "#EEE8DE",
    "list.dropBackground": "#EEE8DE",
    "list.focusBackground": "#E6E0D6",
    "list.focusForeground": "#4B4B4B",
    "menu.background": "#F7F4EE",
    "menu.foreground": "#6E7580",
    "menu.selectionBackground": "#E6E0D6",
    "menu.selectionForeground": "#4B4B4B",
    "sideBar.background": "#F7F4EE",
    "sideBar.foreground": "#ABB0B6",
    "sideBarSectionHeader.background": "#E8E2D8",
    "sideBarSectionHeader.foreground": "#ABB0B6",
    "activityBar.background": "#F0EBE2",
    "activityBar.foreground": "#ED9366",
    "activityBar.inactiveForeground": "#ABB0B6",
    "titleBar.activeBackground": "#F0EBE2",
    "titleBar.activeForeground": "#6E7580",
    "statusBar.background": "#E8E2D8",
    "statusBar.foreground": "#0095B3",
    "statusBar.noFolderBackground": "#E8E2D8",
    "statusBar.debuggingBackground": "#E8E2D8",
    "statusBarItem.hoverBackground": "#DED6C9",
    "statusBarItem.activeBackground": "#D2C8BA",
    "tab.activeBackground": "#F7F4EE",
    "tab.inactiveBackground": "#EDE7DD",
    "tab.activeForeground": "#6E7580",
    "tab.inactiveForeground": "#ABB0B6",
    "tab.border": "#D2C8BA",
    "tab.activeBorder": "#0095B3",
    "panel.background": "#F7F4EE",
    "panel.border": "#D2C8BA",
    "editorWidget.background": "#FFFDF8",
    "editorWidget.border": "#D2C8BA",
    "input.background": "#FFFFFF",
    "input.foreground": "#A37ACC",
    "input.border": "#D2C8BA",
    "dropdown.background": "#FFFFFF",
    "dropdown.foreground": "#A37ACC",
    "dropdown.border": "#D2C8BA",
    "badge.background": "#ED9366",
    "badge.foreground": "#000000",
    "button.background": "#ED9366",
    "button.foreground": "#000000",
    "button.hoverBackground": "#FF5500",
    "progressBar.background": "#ED9366",
    "minimap.border": "#ED9366",
    "scrollbarSlider.background": "#2F8D6E80",
    "scrollbarSlider.hoverBackground": "#2F8D6EAA",
    "scrollbarSlider.activeBackground": "#2F8D6ECC",
    "terminal.background": "#F7F4EE",
    "terminal.foreground": "#6E7580",
    "terminalCursor.foreground": "#ED9366",
    "terminalCursor.background": "#ED9366",
    "editorError.foreground": "#FF1919",
    "editorWarning.foreground": "#FFA352",
    "editorInfo.foreground": "#0095B3"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#8C8C8C",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords and operators",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator",
        "keyword.other",
        "punctuation.definition",
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.accessor"
      ],
      "settings": {
        "foreground": "#ED9366"
      }
    },
    {
      "name": "Section punctuation",
      "scope": [
        "punctuation.section"
      ],
      "settings": {
        "foreground": "#6E7580"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted"
      ],
      "settings": {
        "foreground": "#B3AA59"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.other",
        "support.constant"
      ],
      "settings": {
        "foreground": "#0095B3"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#A37ACC"
      }
    },
    {
      "name": "Function variables and macros",
      "scope": [
        "variable.function",
        "support.function",
        "support.macro"
      ],
      "settings": {
        "foreground": "#ED9366",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Annotations",
      "scope": [
        "variable.annotation"
      ],
      "settings": {
        "foreground": "#FFA352",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Language and parameters",
      "scope": [
        "variable.language",
        "variable.parameter",
        "variable.member"
      ],
      "settings": {
        "foreground": "#0095B3",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage",
        "storage.type"
      ],
      "settings": {
        "foreground": "#6E7580",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Modifiers and types",
      "scope": [
        "storage.modifier",
        "support.type"
      ],
      "settings": {
        "foreground": "#5E8A63",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Support classes",
      "scope": [
        "support.class"
      ],
      "settings": {
        "foreground": "#5E8A63",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Entities",
      "scope": [
        "entity.name"
      ],
      "settings": {
        "foreground": "#5E8A63"
      }
    },
    {
      "name": "Namespaces",
      "scope": [
        "entity.name.namespace"
      ],
      "settings": {
        "foreground": "#A37ACC"
      }
    },
    {
      "name": "Inherited classes",
      "scope": [
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#0095B3",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Invalid and deprecated",
      "scope": [
        "invalid",
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#FF1919",
        "background": "#FF191933"
      }
    },
    {
      "name": "Tags",
      "scope": [
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#ED9366"
      }
    },
    {
      "name": "Tag punctuation",
      "scope": [
        "punctuation.definition.tag.end",
        "punctuation.definition.tag.begin",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#ABB0B6",
        "fontStyle": ""
      }
    },
    {
      "name": "Attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#5E8A63",
        "fontStyle": ""
      }
    },
    {
      "name": "Headings",
      "scope": [
        "markup.heading",
        "markup.heading punctuation.definition.heading",
        "markup.heading.1 punctuation.definition.heading"
      ],
      "settings": {
        "foreground": "#ED9366",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Links",
      "scope": [
        "string.other.link",
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#0095B3"
      }
    },
    {
      "name": "Markup bold/italic",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup bold+italic",
      "scope": [
        "markup.italic markup.bold",
        "markup.bold markup.italic"
      ],
      "settings": {
        "fontStyle": "bold italic"
      }
    },
    {
      "name": "Markup hr",
      "scope": [
        "punctuation.definition.thematic-break"
      ],
      "settings": {
        "foreground": "#ED9366"
      }
    },
    {
      "name": "Lists and quotes",
      "scope": [
        "markup.list.numberOrange.bullet",
        "markup.quote punctuation.definition.blockquote",
        "markup.list punctuation.definition.list_item"
      ],
      "settings": {
        "foreground": "#0095B3"
      }
    },
    {
      "name": "Markup code block",
      "scope": [
        "markup.raw"
      ],
      "settings": {
        "foreground": "#6E7580",
        "background": "#EEE8DE"
      }
    },
    {
      "name": "Markup inline code",
      "scope": [
        "markup.raw.inline"
      ],
      "settings": {
        "foreground": "#A37ACC",
        "background": "#0095B333"
      }
    },
    {
      "name": "Text punctuation emphasis",
      "scope": [
        "text punctuation.definition.italic",
        "text punctuation.definition.bold"
      ],
      "settings": {
        "foreground": "#A37ACC"
      }
    },
    {
      "name": "Diff metadata",
      "scope": [
        "meta.diff",
        "meta.diff.header",
        "markup.deleted",
        "markup.inserted",
        "markup.changed"
      ],
      "settings": {
        "foreground": "#ED9366"
      }
    },
    {
      "name": "YAML tags",
      "scope": [
        "entity.name.tag.yaml"
      ],
      "settings": {
        "foreground": "#ED9366"
      }
    },
    {
      "name": "YAML unquoted strings",
      "scope": [
        "source.yaml string.unquoted"
      ],
      "settings": {
        "foreground": "#5E8A63"
      }
    },
    {
      "name": "SystemVerilog module storage",
      "scope": [
        "source.systemverilog storage.type.module"
      ],
      "settings": {
        "foreground": "#B3AA59",
        "fontStyle": "italic"
      }
    },
    {
      "name": "SystemVerilog module name",
      "scope": [
        "source.systemverilog entity.name.type.module"
      ],
      "settings": {
        "foreground": "#5E8A63"
      }
    },
    {
      "name": "SystemVerilog bind params",
      "scope": [
        "source.systemverilog meta.module.inst meta.bind.param support.function.port"
      ],
      "settings": {
        "foreground": "#A37ACC",
        "fontStyle": "italic"
      }
    },
    {
      "name": "SystemVerilog ports",
      "scope": [
        "source.systemverilog support.function.port"
      ],
      "settings": {
        "foreground": "#ED9366",
        "fontStyle": "italic"
      }
    },
    {
      "name": "SystemVerilog system functions",
      "scope": [
        "source.systemverilog support.function.system"
      ],
      "settings": {
        "foreground": "#A37ACC"
      }
    },
    {
      "name": "Linter errors",
      "scope": [
        "markup.error.sublime_linter"
      ],
      "settings": {
        "foreground": "#FF1919"
      }
    },
    {
      "name": "Linter warnings",
      "scope": [
        "markup.warning.sublime_linter"
      ],
      "settings": {
        "foreground": "#FFA352"
      }
    },
    {
      "name": "Linter filenames",
      "scope": [
        "entity.name.filename.sublime_linter"
      ],
      "settings": {
        "foreground": "#6E7580"
      }
    },
    {
      "name": "Linter labels",
      "scope": [
        "entity.name.label.linter-name.sublime_linter"
      ],
      "settings": {
        "foreground": "#5E8A63"
      }
    },
    {
      "name": "Linter error codes",
      "scope": [
        "entity.name.class.error-code.sublime_linter"
      ],
      "settings": {
        "foreground": "#0095B3"
      }
    },
    {
      "name": "Unclosed JSON string",
      "scope": [
        "invalid.illegal.unclosed-string.json"
      ],
      "settings": {
        "background": "#F7F4EE"
      }
    }
  ]
}
