{
  "name": "Gunacode Dark",
  "type": "dark",
  "colors": {
    "foreground": "#D9D5CC",
    "descriptionForeground": "#677A83",
    "errorForeground": "#FF0000",
    "focusBorder": "#F2B076",
    "selection.background": "#3A5166",
    "editor.background": "#161C23",
    "editor.foreground": "#D9D5CC",
    "editorLineNumber.foreground": "#515F66",
    "editorLineNumber.activeForeground": "#D9D5CC",
    "editorCursor.foreground": "#F2B076",
    "editor.selectionBackground": "#3A5166",
    "editor.selectionHighlightBackground": "#F2B076CC",
    "editor.inactiveSelectionBackground": "#1E2A35",
    "editor.lineHighlightBackground": "#1E2630",
    "editor.wordHighlightBackground": "#F2B076CC",
    "editor.wordHighlightStrongBackground": "#F2B076CC",
    "editor.findMatchBackground": "#F2E6A9",
    "editor.findMatchForeground": "#000000",
    "editor.findMatchHighlightBackground": "#F2E6A966",
    "editor.findRangeHighlightBackground": "#F2E6A933",
    "editorBracketMatch.border": "#FF0000",
    "editorBracketMatch.background": "#FF00001A",
    "editorIndentGuide.background": "#20272E",
    "editorIndentGuide.activeBackground": "#AAFF9954",
    "editorWhitespace.foreground": "#20272E",
    "editorGutter.background": "#161C23",
    "editorGutter.added": "#B5F2A9",
    "editorGutter.modified": "#7ED4E5",
    "editorGutter.deleted": "#F2766B",
    "diffEditor.insertedTextBackground": "hsla(180, 45%, 60%, 0.15)",
    "diffEditor.insertedLineBackground": "hsla(180, 45%, 60%, 0.15)",
    "diffEditor.removedTextBackground": "hsla(357, 45%, 60%, 0.15)",
    "diffEditor.removedLineBackground": "hsla(357, 45%, 60%, 0.15)",
    "gitDecoration.addedResourceForeground": "#B5F2A9",
    "gitDecoration.modifiedResourceForeground": "#7ED4E5",
    "gitDecoration.deletedResourceForeground": "#F2766B",
    "gitDecoration.untrackedResourceForeground": "#B5F2A9",
    "gitDecoration.ignoredResourceForeground": "#677A83",
    "list.activeSelectionBackground": "#1E2A35",
    "list.inactiveSelectionBackground": "#1E2A35",
    "list.hoverBackground": "#1E2630",
    "list.dropBackground": "#1E2630",
    "sideBar.background": "#161C23",
    "sideBar.foreground": "#D9D5CC",
    "sideBarSectionHeader.background": "#1E2630",
    "sideBarSectionHeader.foreground": "#D9D5CC",
    "activityBar.background": "#161C23",
    "activityBar.foreground": "#F2D7BB",
    "activityBar.inactiveForeground": "#677A83",
    "titleBar.activeBackground": "#161C23",
    "titleBar.activeForeground": "#D9D5CC",
    "statusBar.background": "#161C23",
    "statusBar.foreground": "#D9D5CC",
    "statusBar.noFolderBackground": "#161C23",
    "statusBar.debuggingBackground": "#161C23",
    "statusBarItem.hoverBackground": "#1C232C",
    "statusBarItem.activeBackground": "#20272E",
    "tab.activeBackground": "#161C23",
    "tab.inactiveBackground": "#1C232C",
    "tab.activeForeground": "#D9D5CC",
    "tab.inactiveForeground": "#677A83",
    "tab.border": "#20272E",
    "tab.activeBorder": "#8DB386",
    "panel.background": "#161C23",
    "panel.border": "#20272E",
    "editorWidget.background": "#1C232C",
    "editorWidget.border": "#20272E",
    "input.background": "#1C232C",
    "input.foreground": "#D9D5CC",
    "input.border": "#20272E",
    "dropdown.background": "#1C232C",
    "dropdown.foreground": "#D9D5CC",
    "dropdown.border": "#20272E",
    "badge.background": "#F2B076",
    "badge.foreground": "#000000",
    "button.background": "#F2B076",
    "button.foreground": "#000000",
    "button.hoverBackground": "#F2766B",
    "progressBar.background": "#F2B076",
    "minimap.border": "#F2B076",
    "scrollbarSlider.background": "#20272E80",
    "scrollbarSlider.hoverBackground": "#20272EAA",
    "scrollbarSlider.activeBackground": "#20272ECC",
    "terminal.background": "#161C23",
    "terminal.foreground": "#D9D5CC",
    "terminalCursor.foreground": "#F2B076",
    "terminalCursor.background": "#F2B076",
    "editorError.foreground": "#FF0000",
    "editorWarning.foreground": "#F2E6A9",
    "editorInfo.foreground": "#7ED4E5"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#677A83",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords and operators",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator",
        "keyword.other",
        "punctuation.definition",
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.accessor"
      ],
      "settings": {
        "foreground": "#F2B076"
      }
    },
    {
      "name": "Section punctuation",
      "scope": [
        "punctuation.section"
      ],
      "settings": {
        "foreground": "#D9D5CC"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted"
      ],
      "settings": {
        "foreground": "#F2E6A9"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.other",
        "support.constant"
      ],
      "settings": {
        "foreground": "#33B4CE"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#F26091"
      }
    },
    {
      "name": "Function variables and macros",
      "scope": [
        "variable.function",
        "support.function",
        "support.macro"
      ],
      "settings": {
        "foreground": "#F2766B",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Annotations",
      "scope": [
        "variable.annotation"
      ],
      "settings": {
        "foreground": "#F2766B",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Language and parameters",
      "scope": [
        "variable.language",
        "variable.parameter",
        "variable.member"
      ],
      "settings": {
        "foreground": "#7ED4E5",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage",
        "storage.type"
      ],
      "settings": {
        "foreground": "#F2D7BB",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Modifiers and types",
      "scope": [
        "storage.modifier",
        "support.type"
      ],
      "settings": {
        "foreground": "#F2E6A9",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Support classes",
      "scope": [
        "support.class"
      ],
      "settings": {
        "foreground": "#B5F2A9",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Entities",
      "scope": [
        "entity.name"
      ],
      "settings": {
        "foreground": "#B5F2A9"
      }
    },
    {
      "name": "Namespaces",
      "scope": [
        "entity.name.namespace"
      ],
      "settings": {
        "foreground": "#F2766B"
      }
    },
    {
      "name": "Inherited classes",
      "scope": [
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#7ED4E5",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Invalid and deprecated",
      "scope": [
        "invalid",
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#D9D5CC",
        "background": "#7ED4E580"
      }
    },
    {
      "name": "Tags",
      "scope": [
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#F2D7BBE6"
      }
    },
    {
      "name": "Tag punctuation",
      "scope": [
        "punctuation.definition.tag.end",
        "punctuation.definition.tag.begin",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#F2D7BB73",
        "fontStyle": ""
      }
    },
    {
      "name": "Attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#B5F2A9E6",
        "fontStyle": ""
      }
    },
    {
      "name": "Headings",
      "scope": [
        "markup.heading",
        "markup.heading punctuation.definition.heading",
        "markup.heading.1 punctuation.definition.heading"
      ],
      "settings": {
        "foreground": "#F2B076",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Links",
      "scope": [
        "string.other.link",
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#33B4CE"
      }
    },
    {
      "name": "Markup bold/italic",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup bold+italic",
      "scope": [
        "markup.italic markup.bold",
        "markup.bold markup.italic"
      ],
      "settings": {
        "fontStyle": "bold italic"
      }
    },
    {
      "name": "Markup hr",
      "scope": [
        "punctuation.definition.thematic-break"
      ],
      "settings": {
        "foreground": "#F2B076"
      }
    },
    {
      "name": "Lists and quotes",
      "scope": [
        "markup.list.numberOrange.bullet",
        "markup.quote punctuation.definition.blockquote",
        "markup.list punctuation.definition.list_item"
      ],
      "settings": {
        "foreground": "#7ED4E5"
      }
    },
    {
      "name": "Markup code block",
      "scope": [
        "markup.raw"
      ],
      "settings": {
        "foreground": "#C6CED2",
        "background": "#1E262F"
      }
    },
    {
      "name": "Markup inline code",
      "scope": [
        "markup.raw.inline"
      ],
      "settings": {
        "foreground": "#F26091",
        "background": "#33B4CE80"
      }
    },
    {
      "name": "Text punctuation emphasis",
      "scope": [
        "text punctuation.definition.italic",
        "text punctuation.definition.bold"
      ],
      "settings": {
        "foreground": "#F26091"
      }
    },
    {
      "name": "Diff metadata",
      "scope": [
        "meta.diff",
        "meta.diff.header",
        "markup.deleted",
        "markup.inserted",
        "markup.changed"
      ],
      "settings": {
        "foreground": "#F2B076"
      }
    },
    {
      "name": "YAML tags",
      "scope": [
        "entity.name.tag.yaml"
      ],
      "settings": {
        "foreground": "#F2B076"
      }
    },
    {
      "name": "YAML unquoted strings",
      "scope": [
        "source.yaml string.unquoted"
      ],
      "settings": {
        "foreground": "#B5F2A9"
      }
    },
    {
      "name": "SystemVerilog module storage",
      "scope": [
        "source.systemverilog storage.type.module"
      ],
      "settings": {
        "foreground": "#F2E6A9",
        "fontStyle": "italic"
      }
    },
    {
      "name": "SystemVerilog module name",
      "scope": [
        "source.systemverilog entity.name.type.module"
      ],
      "settings": {
        "foreground": "#B5F2A9"
      }
    },
    {
      "name": "SystemVerilog bind params",
      "scope": [
        "source.systemverilog meta.module.inst meta.bind.param support.function.port"
      ],
      "settings": {
        "foreground": "#F26091",
        "fontStyle": "italic"
      }
    },
    {
      "name": "SystemVerilog ports",
      "scope": [
        "source.systemverilog support.function.port"
      ],
      "settings": {
        "foreground": "#F2766B",
        "fontStyle": "italic"
      }
    },
    {
      "name": "SystemVerilog system functions",
      "scope": [
        "source.systemverilog support.function.system"
      ],
      "settings": {
        "foreground": "#F26091"
      }
    },
    {
      "name": "Linter errors",
      "scope": [
        "markup.error.sublime_linter"
      ],
      "settings": {
        "foreground": "#FF0000"
      }
    },
    {
      "name": "Linter warnings",
      "scope": [
        "markup.warning.sublime_linter"
      ],
      "settings": {
        "foreground": "#F2E6A9"
      }
    },
    {
      "name": "Linter filenames",
      "scope": [
        "entity.name.filename.sublime_linter"
      ],
      "settings": {
        "foreground": "#F2D7BB"
      }
    },
    {
      "name": "Linter labels",
      "scope": [
        "entity.name.label.linter-name.sublime_linter"
      ],
      "settings": {
        "foreground": "#B5F2A9"
      }
    },
    {
      "name": "Linter error codes",
      "scope": [
        "entity.name.class.error-code.sublime_linter"
      ],
      "settings": {
        "foreground": "#7ED4E5"
      }
    },
    {
      "name": "Unclosed JSON string",
      "scope": [
        "invalid.illegal.unclosed-string.json"
      ],
      "settings": {
        "background": "#161C23"
      }
    }
  ]
}
